$ var tabs = input.tabs;
<div class="tabs">
    <ul class="nav nav-tabs">
        <for|tab| of=tabs>
            <li class=tab.liClass>
                <a href=`#${tab.id}` data-toggle="tab">${tab.title}</a>
            </li>
        </for>
    </ul>
    <div class="tab-content">
        <for|tab| of=tabs>
            <div id=tab.id class=tab.divClass>
                <${tab}/>
            </div>
        </for>
    </div>
</div>